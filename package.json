		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"stylelint": "^16.13.2",
		"stylelint-config-recommended": "^15.0.0",
		"stylelint-config-standard": "^37.0.0",
		"stylelint-gamut": "^1.3.4",
		"svelte": "^5.19.0",
		"svelte-check": "^4.1.4",
		"tslib": "^2.8.1",
		"typescript": "^5.7.3",

		"@sveltejs/adapter-auto": "^4.0.0",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@trivago/prettier-plugin-sort-imports": "^5.2.1",
		"@typescript-eslint/eslint-plugin": "^8.20.0",
		"@typescript-eslint/parser": "^8.20.0",
		"autoprefixer": "^10.4.20",
		"commitlint": "^19.6.1",
		"eslint": "^9.18.0",
